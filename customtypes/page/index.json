{
  "id": "page",
  "label": "Page",
  "format": "page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": { "label": "UID", "placeholder": "URL slug for the page" }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Page Title",
          "placeholder": "Title for the page",
          "allowTargetBlank": false,
          "single": "paragraph"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "image": { "type": "SharedSlice" },
            "quote": { "type": "SharedSlice" },
            "text": { "type": "SharedSlice" },
            "contact_form": { "type": "SharedSlice" },
            "book_appointment": { "type": "SharedSlice" },
            "treatment_list": { "type": "SharedSlice" },
            "hero_home": { "type": "SharedSlice" },
            "employee_listing": { "type": "SharedSlice" },
            "hero": { "type": "SharedSlice" }
          }
        }
      }
    }
  }
}
