{
  "id": "settings",
  "label": "Settings",
  "format": "custom",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "name": {
        "type": "StructuredText",
        "config": {
          "label": "Name",
          "placeholder": "Name of your blog (e.g. your name)",
          "allowTargetBlank": false,
          "single": "heading1"
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "Short description of your blog",
          "allowTargetBlank": false,
          "single": "paragraph"
        }
      },
      "profilePicture": {
        "type": "Image",
        "config": {
          "label": "Profile Picture",
          "constraint": { "width": 2000, "height": 2000 },
          "thumbnails": []
        }
      }
    },
    "Locations": {
      "email": {
        "type": "Text",
        "config": { "label": "Email", "placeholder": "" }
      },
      "locations": {
        "type": "Group",
        "config": {
          "label": "Location",
          "fields": {
            "address": {
              "type": "StructuredText",
              "config": {
                "label": "address",
                "placeholder": "",
                "allowTargetBlank": true,
                "single": "strong,paragraph"
              }
            },
            "phone": {
              "type": "Text",
              "config": { "label": "Phone", "placeholder": "" }
            },
            "booking_link": {
              "type": "Link",
              "config": {
                "label": "Booking Link",
                "placeholder": "",
                "allowTargetBlank": true,
                "select": null
              }
            },
            "booking_label": {
              "type": "Text",
              "config": { "label": "Booking Label", "placeholder": "" }
            }
          }
        }
      }
    }
  }
}
